{
  "mcpServers": {
    "pixellab": {
      "url": "https://api.pixellab.ai/mcp",
      "transport": "http",
      "headers": {
        "Authorization": "Bearer YOUR_PIXELLAB_API_KEY"
      }
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem"
      ],
      "env": {
        "ALLOWED_DIRECTORIES": "/Users/trislit/Documents/GitHub/BNBBallonpump"
      }
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "your-github-token-if-you-have-one"
      }
    },
    "vercel": {
      "transport": "streamableHttp",
      "url": "https://mcp.vercel.com"
    },
    "solana-developer": {
      "transport": "sse",
      "url": "https://mcp.solana.com/mcp"
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "playwright-mcp"
      ]
    },
    "tatumio": {
      "command": "npx",
      "args": [
        "-c",
        "TATUM_API_KEY=YOUR_TATUM_API_KEY npx @tatumio/blockchain-mcp"
      ]
    },
    "hf-mcp-server": {
      "transport": "http",
      "url": "https://huggingface.co/mcp",
      "headers": {
        "Authorization": "Bearer YOUR_HUGGINGFACE_API_KEY"
      }
    },
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--project-ref=uvmfrbiojefvtbfgbcfk"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "YOUR_SUPABASE_ACCESS_TOKEN"
      }
    },
    "endgame": {
      "transport": "sse",
      "url": "https://mcp.endgame.dev"
    },
    "gibsonai": {
      "transport": "sse",
      "url": "https://mcp.gibsonai.com"
    },
    "augments": {
      "command": "npx",
      "args": [
        "-y",
        "@augmnt/mcp-server"
      ]
    },
    "shrimp": {
      "command": "npx",
      "args": [
        "-y",
        "@shrimp-ai/task-manager-mcp"
      ]
    }
  }
}


